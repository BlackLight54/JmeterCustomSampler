{
    "name": "cbdc-notls-ca-network",
    "version": "1.0.0",
    "client": {
        "organization": "FIOrg",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                },
                "orderer": "300"
            }
        }
    },
    "channels": {
        "epengo-channel": {
            "orderers": [
                "orderer0.example.com"
            ],
            "peers": {
                "peer0.centralbank.example.com": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer0.fi.example.com": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                }
            }
        }
    },
    "organizations": {
        "CentralBankOrg": {
            "mspid": "CentralBankOrgMSP",
            "peers": [
                "peer0.centralbank.example.com"
            ],
            "certificateAuthorities": [
                "ca-centralbank"
            ],
            "adminPrivateKey": {
                "path": "crypto-config/peerOrganizations/centralbank.example.com/users/Admin@centralbank.example.com/msp/keystore/key.pem"
            },
            "signedCert": {
                "path": "crypto-config/peerOrganizations/centralbank.example.com/users/Admin@centralbank.example.com/msp/signcerts/Admin@centralbank.example.com-cert.pem"
            }
        },
        "FIOrg": {
            "mspid": "FIOrgMSP",
            "peers": [
                "peer0.fi.example.com"
            ],
            "certificateAuthorities": [
                "ca-fi"
            ],
            "adminPrivateKey": {
                "path": "crypto-config/peerOrganizations/fi.example.com/users/Admin@fi.example.com/msp/keystore/key.pem"
            },
            "signedCert": {
                "path": "crypto-config/peerOrganizations/fi.example.com/users/Admin@fi.example.com/msp/signcerts/Admin@fi.example.com-cert.pem"
            }
        }
    },
    "orderers": {
        "orderer0.example.com": {
            "url": "grpc://localhost:7050"
        }
    },
    "peers": {
        "peer0.centralbank.example.com": {
            "url": "grpc://localhost:7051"
        },
        "peer0.fi.example.com": {
            "url": "grpc://localhost:7051"
        }
    },
    "certificateAuthorities": {
        "ca-centralbank": {
          "url": "grpc://localhost:7054",
          "httpOptions": {
            "verify": false
          },
          "registrar": [
            {
              "enrollId": "admin",
              "enrollSecret": "adminpw"
            }
          ],
          "caName": "ca-centralbank"
        },
        "ca-fi": {
          "url": "grpc://localhost:7054",
          "httpOptions": {
            "verify": false
          },
          "registrar": [
            {
              "enrollId": "admin",
              "enrollSecret": "adminpw"
            }
          ],
          "caName": "ca-fi"
        }
      }
}